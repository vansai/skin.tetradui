<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<variable name="ListItemPosterVar">
		<value condition="!String.IsEmpty(ListItem.Art(season.poster))">$INFO[ListItem.Art(season.poster)]</value>
		<value condition="!String.IsEmpty(ListItem.Art(tvshow.poster))">$INFO[ListItem.Art(tvshow.poster)]</value>
		<value condition="!String.IsEmpty(ListItem.Art(poster))">$INFO[ListItem.Art(poster)]</value>
		<value condition="!String.IsEmpty(ListItem.Art(thumb))">$INFO[ListItem.Art(thumb)]</value>
		<value>$INFO[ListItem.Icon]</value>
	</variable>
	<variable name="ListItemBannerVar">
		<value condition="!String.IsEmpty(ListItem.Art(season.banner))">$INFO[ListItem.Art(season.banner)]</value>
		<value condition="!String.IsEmpty(ListItem.Art(tvshow.banner))">$INFO[ListItem.Art(tvshow.banner)]</value>
		<value condition="!String.IsEmpty(ListItem.Art(banner))">$INFO[ListItem.Art(banner)]</value>
		<value condition="!String.IsEmpty(ListItem.Art(thumb))">$INFO[ListItem.Art(thumb)]</value>
		<value>$INFO[ListItem.Icon]</value>
	</variable>
	<variable name="PlayerPosterVar">
		<value condition="!String.IsEmpty(Player.Art(tvshow.poster))">$INFO[Player.Art(tvshow.poster)]</value>
		<value condition="!String.IsEmpty(Player.Art(poster))">$INFO[Player.Art(poster)]</value>
		<value condition="!String.IsEmpty(Player.Art(thumb))">$INFO[Player.Art(thumb)]</value>
		<value>$INFO[Player.Icon]</value>
	</variable>
	<variable name="SeekBarLabels">
		<value condition="Player.Caching">$LOCALIZE[31234] $INFO[Player.CacheLevel]%</value>
		<value condition="!String.IsEmpty(Player.SeekStepSize)">$LOCALIZE[773] $INFO[Player.SeekStepSize]</value>
		<value condition="!String.IsEmpty(Player.SeekOffset) + Player.DisplayAfterSeek">$LOCALIZE[773] $INFO[Player.SeekOffset]</value>
		<value condition="Player.Paused">$LOCALIZE[31231]</value>
		<value condition="Player.Forwarding2x">$LOCALIZE[31232] x2</value>
		<value condition="Player.Forwarding4x">$LOCALIZE[31232] x4</value>
		<value condition="Player.Forwarding8x">$LOCALIZE[31232] x8</value>
		<value condition="Player.Forwarding16x">$LOCALIZE[31232] x16</value>
		<value condition="Player.Forwarding32x">$LOCALIZE[31232] x32</value>
		<value condition="Player.Rewinding2x">$LOCALIZE[31233] x2</value>
		<value condition="Player.Rewinding4x">$LOCALIZE[31233] x4</value>
		<value condition="Player.Rewinding8x">$LOCALIZE[31233] x8</value>
		<value condition="Player.Rewinding16x">$LOCALIZE[31233] x16</value>
		<value condition="Player.Rewinding32x">$LOCALIZE[31233] x32</value>
		<value condition="Player.IsTempo">$LOCALIZE[31229] $INFO[Player.PlaySpeed]</value>
		<value>$LOCALIZE[31230]</value>
	</variable>
	<variable name="TimeLabels">
		<value condition="Player.Seeking">$INFO[Player.SeekTime]</value>
		<value>$INFO[VideoPlayer.Time]</value>
	</variable>
	<variable name="DialogVideoInfoTitleLabel">
		<value condition="Container.Content(Movies)">$LOCALIZE[13346]</value>
		<value condition="Container.Content(TVShows)">$LOCALIZE[20351]</value>
		<value condition="Container.Content(Episodes)">$LOCALIZE[20352]</value>
		<value condition="Container.Content(MusicVideos)">$LOCALIZE[20393]</value>
		<value>$LOCALIZE[20159]</value>
	</variable>
	<variable name="StarRating">
		<value condition="Integer.IsLess(ListItem.Rating,1)">flagging/starrating/0.png</value>
		<value condition="Integer.IsLess(ListItem.Rating,2)">flagging/starrating/1.png</value>
		<value condition="Integer.IsLess(ListItem.Rating,3)">flagging/starrating/2.png</value>
		<value condition="Integer.IsLess(ListItem.Rating,4)">flagging/starrating/3.png</value>
		<value condition="Integer.IsLess(ListItem.Rating,5)">flagging/starrating/4.png</value>
		<value condition="Integer.IsLess(ListItem.Rating,6)">flagging/starrating/5.png</value>
		<value condition="Integer.IsLess(ListItem.Rating,7)">flagging/starrating/6.png</value>
		<value condition="Integer.IsLess(ListItem.Rating,8)">flagging/starrating/7.png</value>
		<value condition="Integer.IsLess(ListItem.Rating,9)">flagging/starrating/8.png</value>
		<value condition="Integer.IsLess(ListItem.Rating,10)">flagging/starrating/9.png</value>
		<value>flagging/starrating/10.png</value>
	</variable>
</includes>
